for (let i = 0; i <= 5; i++) {
    setTimeout(() => console.log(i), i * 1000)
}


const makeCounter = step => {
    let count = 0;

    return () => {
        const tmp = count;
        count += step;
        return tmp
    }
};
let a = makeCounter(5);
let b = makeCounter(3);
console.log(a());


